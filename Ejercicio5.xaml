<Window x:Class="Asignacion3Aplicada_1.Ejercicio5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Asignacion3Aplicada_1"
        mc:Ignorable="d"
        Title="Ejercicio5" Height="350" Width="500">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <Label Content= "Elementos"/>
                <TextBox x:Name="ElementostextBox" Height="20" Width="70"/>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Button x:Name="AgregarButton" Content="Agregar" Height="20" Width="70" Margin="0,10" Click="AgregarButton_Click"/>
                <Button x:Name="ImprimirButton" Content="Imprimir" Height="20" Width="70" Margin="10" Click="ImprimirButton_Click"/>
            </StackPanel>
            <StackPanel Orientation="Vertical">
                <ListBox x:Name="ElementosListbox" Height="200" Width="300">
                    
                </ListBox>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
